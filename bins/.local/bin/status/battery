#!/usr/bin/sh

[ 0 -eq "$(cat /sys/class/power_supply/ACAD/online)" ] || echo ﮣ
[ -f /sys/class/power_supply/BAT?/capacity ] ||  echo "" ; exit

for x in /sys/class/power_supply/BAT?/capacity;
do
case "$(cat $x)" in
    [0-9])		echo "" ;;
    1[0-9])		echo "" ;;
    2[0-9])		echo "" ;;
    3[0-9])		echo "" ;;
    4[0-9])		echo "" ;;
    5[0-9])		echo "" ;;
    6[0-9])		echo "" ;;
    7[0-9])		echo "" ;;
    8[0-9])		echo "" ;;
    9[0-9])		echo "" ;;
    *)		    echo "" ;;
esac
done
